# Copy this file to .env and fill in your values

# Device settings (use "mps" for Mac M1/M2, "cuda" for NVIDIA GPU, "cpu" for CPU)
DEVICE=mps

# Hugging Face token (optional, for private models)
HF_TOKEN=

# AWS Configuration (optional, for S3 storage)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
S3_BUCKET_NAME=

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Logging
LOG_LEVEL=INFO
